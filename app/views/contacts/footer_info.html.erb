        <style>
         .page-footer {
          background-color: white;
         }
        </style>


        <br>
        <footer class="page-footer">
          <div class="container">
            <div class="row">
              <div class="col l6 s12">
                <h5 class="black-text">Footer Content</h5>
                <p class="grey-text">You can use rows and columns here to organize your footer content.</p>
              </div>
              <div class="col l4 offset-l2 s12">
                <h5 class="black-text">Links</h5>
                <ul>
                      <li class="collection-item">
                        <% if @contact.vk.present? %>               
                            <%= link_to url_with_protocol(@contact.vk), class: "btn btn-social-icon btn-vk", :target => '_blank' do %>
                              <span class="btn btn-social-icon btn-vk fa fa-vk"></span>
                            <% end %>     
                        <% end %>
                        <% if @contact.fb.present? %>          
                            <%= link_to url_with_protocol(@contact.fb), class: "btn btn-social-icon btn-facebook", :target => '_blank' do %>
                              <span class="btn btn-social-icon btn-facebook fa fa-facebook"></span>
                            <% end %>
                        <% end %>
                        <% if @contact.insta.present? %>            
                            <%= link_to url_with_protocol(@contact.insta), class: "btn btn-social-icon btn-instagram", :target => '_blank' do %>
                              <span class="btn btn-social-icon btn-instagram fa fa-instagram"></span>
                            <% end %>
                        <% end %>
                        <% if @contact.twitter.present? %>          
                            <%= link_to url_with_protocol(@contact.twitter), class: "btn btn-social-icon btn-twitter", :target => '_blank' do %>
                              <span class="btn btn-social-icon btn-twitter fa fa-twitter"></span>
                            <% end %>
                        <% end %>
                      </li>
                      <% if @contact.email.present? %>
                        <li class="collection-item">
                          <div><%= @contact.email%> <%= mail_to @contact.email, "send email"%></div>
                        </li>
                      <% end %>
                      <% if @contact.phone.present? %>
                        <li class="collection-item">
                          <div><%= t(:phone)%>: <%= @contact.phone%></div>
                        </li>
                      <% end %>
                      <% if @contact.phone2.present? %>
                        <li class="collection-item">
                          <div><%= t(:phone)%>: <%= @contact.phone2%></div>
                        </li>
                      <% end %>
                      <% if @contact.phone3.present? %>
                        <li class="collection-item">
                          <div><%= t(:phone)%>: <%= @contact.phone3%></div>
                        </li>
                      <% end %>
                      <% if @contact.adress.present? %>
                        <li class="collection-item">
                          <div><%= t(:address)%>: <%= @contact.adress%></div>
                        </li>
                      <% end %>
                      <% if @contact.how_to_get.present? %>        
                        <li class="collection-item">
                          <div><%= t(:how_to_get)%>: <%= @contact.how_to_get%></div>
                        </li>
                      <% end %>
                  
                </ul>
              </div>
            </div>
          </div>
          <div class="footer-copyright">
            <div class="container grey-text">
            created by dumka.maynula
            <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
            </div>
          </div>
        </footer> 